{
  "name": "fortnite-item-shop",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "tsc && xo"
  },
  "dependencies": {
    "bent": "^7.3.12",
    "mem": "^9.0.1",
    "vue": "^3.2.6"
  },
  "devDependencies": {
    "@types/bent": "^7.3.2",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.6",
    "esbuild": "^0.12.28",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "sass": "^1.40.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.7",
    "vue-tsc": "^0.3.0",
    "xo": "^0.38.2",
    "xxhash-addon": "^1.4.0"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "unicorn/import-index": [
        "off"
      ],
      "unicorn/no-array-reduce": [
        "off"
      ],
      "import/extensions": [
        "off"
      ],
      "import/no-useless-path-segments": [
        "off"
      ],
      "unicorn/no-fn-reference-in-iterator": [
        "off"
      ],
      "unicorn/no-reduce": [
        "off"
      ],
      "import/no-named-as-default": [
        "off"
      ],
      "@typescript-eslint/prefer-literal-enum-member": [
        "off"
      ],
      "react/no-array-index-key": [
        "off"
      ],
      "unicorn/no-new-array": [
        "off"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": true,
    "useTabs": false,
    "endOfLine": "lf",
    "trailingComma": "es5"
  }
}
